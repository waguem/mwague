{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/global.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "cypress:open": "cypress open",
    "cypress:test": "cypress run",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:schema:generate": "npx prisma format --schema=./prisma/schema.prisma && npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:schema:migrate:dev": "pnpm run prisma:schema:generate && npx prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:schema:push": "npx prisma db push --schema=./prisma/schema.prisma",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts",
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react-dom": "18.2.4",
    "accept-language": "^3.0.18",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.8",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.12.12",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-resources-to-backend": "^1.1.4",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "postcss": "8.4.23",
    "postcss-focus-visible": "^8.0.2",
    "prisma": "^4.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.3.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "webpack": "^5.84.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-syntax-highlighter": "^15.5.7",
    "babel-loader": "^9.1.2",
    "cypress": "^12.13.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "storybook": "^7.0.18",
    "ts-essentials": "^9.3.2",
    "ts-node": "^10.9.1"
  }
}
